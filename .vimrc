set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
%retab!
set syntax=on
set number
set ignorecase
set hlsearch
set incsearch
set laststatus=2
set showmatch
colorscheme molokai
set t_Co=256
set cursorline
set cursorcolumn
hi CursorLine   cterm=NONE ctermbg=darkred ctermfg=white guibg=darkred guifg=white
hi CursorColumn cterm=NONE ctermbg=darkred ctermfg=white guibg=darkred guifg=white
highlight Comment ctermfg=blue guifg=blue 
